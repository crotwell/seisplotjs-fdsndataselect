{
  "name": "seisplotjs-fdsndataselect",
  "version": "1.1.2-alpha.1",
  "description": "Querying and parsing of output of FDSN DataSelect web service for seismograms.",
  "main": "./lib/fdsndataselect.js",
  "dependencies": {
    "rsvp": "^4.0.1",
    "seisplotjs-miniseed": "^1.2.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "qunit -c ./lib/fdsndataselect.js -t ./test/*.js",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/fdsndataselect.js --standalone seisplotjs_fdsndataselect -o seisplotjs_fdsndataselect_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-fdsndataselect.git"
  },
  "keywords": [
    "seismology",
    "fdsndataselect",
    "dataselect",
    "fdsn",
    "seismogram",
    "miniseed"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-fdsndataselect/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-fdsndataselect",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
